<h1>Listing experiments</h1>

<table class="table table-striped table-bordered table-hover">
  <tr>
    <th>id</th>
    <th>Start Time</th>
  </tr>

<% @experiments.each do |experiment| %>
  <tr>
    <td><%= experiment.id %></td>
    <td><%= link_to experiment.startTime, experiment %></td>
  </tr>
<% end %>
</table>

