<p id="notice"><%= notice %></p>

<% if !@temperatures.empty? %>
	<h3>Temperature</h3>
	<table class="table table-striped table-bordered table-hover"> 
		<tr>
		    <th>Time</th>
		    <th>Value</th>
		    <th>Error</th>
		</tr>

	<% @temperatures.each do |temperature| %>
	  <tr>
	    <td><%= temperature.time %></td>
	    <td><%= temperature.value %></td>
	    <td><%= temperature.error %></td>
	  </tr>
	<% end %>
	</table>
<% end %>

